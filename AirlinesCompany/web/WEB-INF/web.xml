<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <listener>
        <description>RequestListener, RequestAttributeListener</description>
        <listener-class>CommandPost.logic.NewServletListener</listener-class>
    </listener>
    
    <servlet>
        <servlet-name>EncodingFiltr</servlet-name>
        <servlet-class>CommandPost.logic.EncodingFiltr</servlet-class>
    </servlet>
       <servlet>
        <servlet-name>AddUser</servlet-name>
        <servlet-class>CommandPost.logic.AddUser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GeneralServlet</servlet-name>
        <servlet-class>AirLines.controller.GeneralServlet</servlet-class>
    </servlet>    
    <servlet-mapping>
        <servlet-name>AddUser</servlet-name>
        <url-pattern>/AddUser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GeneralServlet</servlet-name>
        <url-pattern>/GeneralServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
